{
    "name": "cima-alfa/nette-di-example",
    "description": "Docker skeleton for web development...",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Stepanek",
            "email": "contact@sjeremy.dev"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-pdo": "*",
        "nette/di": "^3.1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/app"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
